{
  "name": "ecomad-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:atlas": "EXPO_ATLAS=true npx expo start --no-dev",
    "web": "expo start --web",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "android:redmi": "expo run:android --device Redmi_Note_8T",
    "android:release": "expo run:android --variant Release",
    "android:prebuild": "expo prebuild --platform android",
    "ios": "expo run:ios",
    "ios:simulator": "expo run:ios --device 'iPhone 16 Pro'",
    "ios:iphone": "expo run:ios --device 00008030-000818C02252802E",
    "ios:device": "expo run:ios --device",
    "ios:release": "expo run:ios  --configuration Release",
    "ios:prebuild": "expo prebuild --platform ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "eas:build:dev": "eas build --profile dev --platform all",
    "eas:build:staging": "eas build --profile staging --platform all",
    "eas:build:prod": "eas build --profile production --platform all",
    "eas:submit:android": "eas submit -p android",
    "eas:submit:ios": "eas submit -p ios",
    "update:dev": "eas update --channel dev --message \"update dev\"",
    "update:staging": "eas update --channel staging --message \"update staging\"",
    "update:prod": "eas update --channel prod --message \"update prod\"",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:update-snapshots": "jest -u",
    "prepare": "husky"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "~5.2.6",
    "@hugeicons-pro/core-duotone-rounded": "~1.0.14",
    "@hugeicons-pro/core-solid-rounded": "~1.0.14",
    "@hugeicons-pro/core-stroke-rounded": "~1.0.14",
    "@hugeicons/react-native": "~1.0.7",
    "@react-navigation/elements": "~2.8.1",
    "@react-navigation/native": "~7.1.6",
    "@rnmapbox/maps": "~10.2.7",
    "@turf/helpers": "^7.3.0",
    "@turf/turf": "~7.2.0",
    "drizzle-orm": "~0.44.5",
    "expo": "~54.0.23",
    "expo-asset": "~12.0.9",
    "expo-battery": "~10.0.7",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.1",
    "expo-drizzle-studio-plugin": "~0.2.1",
    "expo-file-system": "^19.0.19",
    "expo-font": "~14.0.2",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.1",
    "expo-location": "~19.0.1",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.1",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.1",
    "expo-status-bar": "~3.0.1",
    "expo-system-ui": "~6.0.1",
    "expo-task-manager": "~14.0.8",
    "nativewind": "~4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81",
    "react-native-gesture-handler": "~2.29.1",
    "react-native-popover-view": "~6.1.0",
    "react-native-popup-menu": "~0.18.0",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.18.0",
    "react-native-svg": "~15.15.0",
    "react-native-web": "~0.21.2",
    "react-native-webview": "13.13.5",
    "react-native-worklets": "~0.6.1",
    "zod": "~4.1.7",
    "zustand": "~5.0.7"
  },
  "devDependencies": {
    "@babel/core": "~7.26.0",
    "@biomejs/biome": "2.3.7",
    "@testing-library/react-native": "~13.3.3",
    "@types/geojson": "~7946.0.16",
    "@types/jest": "~30.0.0",
    "@types/react": "~19.0.10",
    "babel-plugin-inline-import": "~3.0.0",
    "babel-plugin-transform-remove-console": "~6.9.4",
    "drizzle-kit": "~0.31.4",
    "expo-atlas": "^0.4.0",
    "husky": "^9.1.7",
    "jest": "~30.1.3",
    "jest-expo": "~54.0.12",
    "tailwindcss": "~3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true,
  "trustedDependencies": [
    "esbuild",
    "unrs-resolver"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "biome check --no-errors-on-unmatched"
    ],
    "*.json": [
      "biome format"
    ]
  }
}
