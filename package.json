{
	"name": "ecomad-app",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"start:android": "expo start --android",
		"start:ios": "expo start --ios",
		"web": "expo start --web",
		"android": "expo run:android",
		"android:device": "expo run:android --device",
		"android:redmi": "expo run:android --device Redmi_Note_8T",
		"android:release": "expo run:android --configuration Release",
		"ios": "expo run:ios",
		"ios:simulator": "expo run:ios --device 'iPhone 16 Pro'",
		"ios:iphone": "expo run:ios --device 00008030-000818C02252802E",
		"ios:device": "expo run:ios --device",
		"ios:release": "expo run:ios  --configuration Release",
		"prebuild": "expo prebuild",
		"prebuild:clean": "expo prebuild --clean",
		"prebuild:ios": "expo prebuild --platform ios",
		"prebuild:android": "expo prebuild --platform android",
		"lint": "expo lint",
		"lint:fix": "expo lint --fix",
		"format": "prettier --write .",
		"eas:build:android": "eas build -p android",
		"eas:build:ios": "eas build -p ios --profile development --local",
		"eas:submit:android": "eas submit -p android",
		"eas:submit:ios": "eas submit -p ios",
		"test": "jest --watchAll",
		"test:ci": "jest --ci --coverage --watchAll=false",
		"test:update-snapshots": "jest -u"
	},
