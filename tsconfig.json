{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"~/*": ["src/*"],
			"@shared/*": ["src/shared/*"],
			"@map/*": ["src/features/map/*"]
		},
		"typeRoots": ["./types", "./node_modules/@types"]
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"nativewind-env.d.ts",
		"types/**/*.d.ts"
	]
}
